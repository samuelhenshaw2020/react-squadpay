{
  "name": "react-squadpay",
  "private": false,
  "version": "1.0.9",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-squadpay": "^1.0.8"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-react": "^2.2.0",
    "eslint": "^8.26.0",
    "eslint-plugin-react": "^7.31.10",
    "husky": "^8.0.1",
    "jsdom": "^20.0.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.0",
    "vite-plugin-dts": "^1.6.6",
    "vitest": "^0.24.3"
  },
  "files": [
    "dist",
    "dist/**/*.ts"
  ],
  "main": "./dist/squad.umd.js",
  "module": "./dist/squad.es.js",
  "exports": {
    "import": "./dist/squad.es.js",
    "require": "./dist/squad.umd.js"
  },
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/samuelhenshaw2020/react-squadpay.git"
  },
  "bugs": {
