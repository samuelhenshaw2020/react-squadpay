    "React",
    "Payment Gateway",
    "Payment",
    "Online Payment",
    "Squad Payment",
    "Squad Online payment"
  ],
  "description": "React GTCO Squad payment gateway",
  "engines": {
    "node": ">=10"
  },
  "homepage": "https://github.com/samuelhenshaw2020/react-squadpay#readme",
